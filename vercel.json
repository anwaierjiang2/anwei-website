{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm install --legacy-peer-deps --force && cd client && npm install --legacy-peer-deps --force",
  "build.env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vercel-adapter.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}