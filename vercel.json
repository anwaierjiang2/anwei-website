{
  "buildCommand": "node vercel-build.js",
  "outputDirectory": "client/build",
  "devCommand": "npm run dev",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vercel-adapter.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    }
  ],
  "functions": {
    "vercel-adapter.js": {
      "includeFiles": "server/**"
    }
  },
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}